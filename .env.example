# PolyLend Protocol - Environment Variables
# Copy this file to .env and fill in the values

# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# RPC URLs
POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/your-api-key
POLYGON_AMOY_RPC_URL=https://polygon-amoy.g.alchemy.com/v2/your-api-key

# Block explorer API key for verification
POLYGONSCAN_API_KEY=your_polygonscan_api_key

# Deployed contract addresses (fill after deployment)
FACTORY_ADDRESS=
ORACLE_ADDRESS=
ROUTER_ADDRESS=

# For CreateVault script
POSITION_ID=
VAULT_NAME=
VAULT_SYMBOL=
PRICE=

# For BatchUpdatePrices script
VAULT_1=
VAULT_2=
PRICE_1=
PRICE_2=
